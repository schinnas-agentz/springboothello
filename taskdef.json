{
  "family": "gs-spring-boot-%SPRING_PROFILE%",
  "containerDefinitions": [
    {
      "image": "%REPOSITORY_URI%:v_%BUILD_NUMBER%",
      "name": "gs-spring-boot-%SPRING_PROFILE%",
      "cpu": 250,
      "memoryReservation": 128,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 5000,
          "hostPort": 0
        }
      ]
    }
  ]
}
