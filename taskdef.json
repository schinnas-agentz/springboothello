{
  "family": "springboothello-%SPRING_PROFILE%",
  "containerDefinitions": [
    {
      "image": "%REPOSITORY_URI%:%SPRING_PROFILE%",
      "name": "springboothello-%SPRING_PROFILE%",
      "cpu": 250,
      "memoryReservation": 128,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 5000,
          "hostPort": 0
        }
      ]
    }
  ]
}
